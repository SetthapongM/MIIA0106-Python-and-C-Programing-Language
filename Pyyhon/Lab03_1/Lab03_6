car = 0

print("1 = Turn left")
print("2 = Turn right")
print("3 = Turn go ahead")
car = int(input("Please enter Answer: "))

if car == 1:
    print("Car has turned left")
elif car == 2:
    print("Car has turned right")
elif car == 3:
    print("Car has gone ahead")
else:
    print("Your car error")